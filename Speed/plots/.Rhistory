Plot1=function(N=1000,seed=0){
par(mfrow=c(2,1))
if(seed>0) set.seed(seed)
y=rnorm(N)
lambda=0.94
sigma=vector(length=N)
sigma[1]=1
for(i in 2:N){
sigma[i]=lambda* sigma[i-1] + (1-lambda)*y[i-1]^2
y[i]=y[i]*sqrt(sigma[i])
